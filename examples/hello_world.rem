
void func() {
    println("enter p or m for 2&2 : ")
    boolean isend = false
    str input = scan()
    while (!isend){
        if (input == "p"){
            println(2+2)
            isend = true
        }elif (input == "m" ) {
            println(2-1)
            isend = true
        }
        else {
            println("dont support operation")
        }
    }
}

func()